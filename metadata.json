{
  "name": "codingfuture-cfmetrics",
  "version": "0.0.1",
  "author": "Andrey Galkin",
  "summary": "Centralized metric collection and monitoring solution.",
  "license": "Apache-2.0",
  "source": "https://github.com/codingfuture/puppet-cfmetrics",
  "project_page": "https://github.com/codingfuture/puppet-cfmetrics",
  "issues_url": "https://github.com/codingfuture/puppet-cfmetrics/issues",
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "8",
        "9"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "16.04",
        "16.10",
        "17.04"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">=4.7.0 <6.0.0"
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 4.24.0 <5.0.0"
    },
    {
      "name": "codingfuture-cfnetwork",
      "version_requirement": ">= 1.0.0 <2.0.0"
    },
    {
      "name": "codingfuture-cfsystem",
      "version_requirement": ">= 1.1.0 <2.0.0"
    },
    {
      "name": "codingfuture-cfdb",
      "version_requirement": ">= 1.0.4 <2.0.0"
    },
    {
      "name": "codingfuture-cflogsink",
      "version_requirement": ">= 1.0.1 <2.0.0"
    },
    {
      "name": "codingfuture-cfweb",
      "version_requirement": ">= 1.1.1 <2.0.0"
    }
  ],
  "data_provider": "hiera"
}
